import java.util.*;
class Prime{
public static void main(String[] args){
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int c=0,r=0;
if(n==0)
c=1;
else
for(int i=2;i<=n/2;i++){
r++;
if(n%i==0){
c++;
break;
}
}
System.out.println(r+" "+c);
if(c==0)
System.out.print("prime");
else
System.out.print("not prime");
}
